{
  "name": "express-server-example",
  "version": "1.0.0",
  "description": "A simple ExpressJS server in TypeScript with nodemon",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx ts-node src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"ts-node\" src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "@types/node": "^18.15.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.0",
    "faker": "^5.5.3",
    "jwks-rsa": "^3.0.1",
    "mongoose": "^7.0.3",
    "redis": "^4.6.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jquery": "^3.5.16",
    "@types/redis": "^4.0.11",
    "nodemon": "^2.0.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
